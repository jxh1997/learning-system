page {
  background: #efefef;

  font-size: 28rpx;
}

#head {
  margin: auto;
  width: 90%;
  background: white;
  height: 450rpx;
  display: flex;
  border-radius: 55rpx;
  box-shadow: 0 0 15rpx #ddd;
  margin-bottom: 30rpx;
}

#head #head-con {
  width: min-content;
  margin: auto;
}

#head #head-con open-data[type="userAvatarUrl"] {
  width: calc(750rpx * 0.2);
  height: calc(750rpx * 0.2);
  display: block;
  border-radius: 50%;
  overflow: hidden;
  border: 6rpx solid white;
}

#head #head-con open-data[type="userNickName"] {
  text-align: center;
  display: block;
  padding-top: 15rpx;
  color: #666;
}

#head #head-con #modify {
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border-radius: 50%;
  position: absolute;
  top: 30rpx;
  right: 60rpx;
  color: #24dd82;
  font-size: 150%;
  z-index: 10;
}

#modify:active {
  background: #efefef;
}

/*main
----------*/
#main .block {
  width: 90%;
  box-shadow: 0 0 15rpx #ddd;
  background: white;

  margin: 20rpx auto;
  min-height: 100rpx;
  border-radius: 25rpx;
  color: #666;
}

#main .block .row {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 15%;


  border-bottom: thin solid #ddd;
}

#main .block .row:active {
  background: #efefef;
}

#main .block .row:last-child {
  border: 0;
}

#main .block .row .icon {
  text-indent: -28rpx;
}

#main .block .row .icon:before {
  position: relative;
  top: 5rpx;
  left: -28rpx;
  font-size: 38rpx;
}

#main .block .row text {
  text-indent: 28rpx;

  width: 100rpx;
  padding-right: 30rpx;
  font-size: 120%;
  float: right;
  color: #999;
}